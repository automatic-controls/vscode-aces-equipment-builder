{
    "comments":{},
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["/**", "*/"],
        ["/*", "*/"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        {"open":"\"", "close":"\"", "notIn":["string","comment"]},
        {"open":"'", "close":"'", "notIn":["string","comment"]},
        {"open":"/*", "close":"*/", "notIn":["string"]},
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        {"open":"\"", "close":"\"", "notIn":["string","comment"]},
        {"open":"'", "close":"'", "notIn":["string","comment"]},
    ],
    "folding":{
        "markers":{
            "start":"^.*?(?:[\\[\\(\\{]|/\\*\\*?)\\s*$",
            "end":"^\\s*(?:[\\]\\)\\}]|\\*?\\*/)"
        }
    }
}